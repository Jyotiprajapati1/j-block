{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "experimental-block/testimonials",
	"title": "Testimonials",
	"description": "Displays customer testimonials in a customizable format, adding credibility to your site.",
	"attributes": {
		"numColumn" : {
			"type" : "number",
			"default" : 2
		}
	},
	"supports": {
		"anchor": true,
		"align": [ "wide", "full" ],
		"html": false
	},
	"textdomain": "testimonials",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
