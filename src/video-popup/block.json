{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "experimental-block/video-popup",
	"version": "0.1.0",
	"title": "Video Popup",
	"icon": "video-alt3",
	"description": "Adds a popup video display, ideal for showcasing videos in a lightbox format.",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"iconColor" : {
			"type" : "string",
			"default" : "#000"
		},
		"videoCode" : {
			"type" : "string",
			"default" : "#"
		},
		"fontSize" : {
			"type" : "number",
			"default" : 22
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "video-popup",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
