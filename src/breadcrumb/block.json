{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "experimental-block/breadcrumb",
	"version": "0.1.0",
	"title": "Breadcrumb",
	"icon": "ellipsis",
	"description": "Displays a breadcrumb trail that shows users their current location within the site, enhancing navigation.",
	"attributes": {
		"seperator": {
			"type": "string",
			"default": ">"
		},
		"textAlign" : {
			"type" : "string",
			"default" : "left"
		},
		"linkUnderLine" : {
			"type" : "boolean",
			"default" : false
		},
		"textSize" : {
			"type" : "number",
			"default" : 16
		},
		"linkColor" : {
			"type" : "string",
			"default" : ""
		},
		"textColor" : {
			"type" : "string",
			"default" : ""
		},
		"separatorColor" : {
			"type" : "string",
			"default" : ""
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "breadcrumb",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
